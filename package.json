      "@types/bcrypt": "^5.0.0",
      "@types/body-parser": "^1.19.2",
      "@types/colors": "^1.2.1",
      "@types/cors": "^2.8.12",
      "@types/dotenv": "^8.2.0",
      "@types/express": "^4.17.13",
      "@types/http-errors": "^1.8.2",
      "@types/jest": "^27.5.2",
      "@types/jsonwebtoken": "^8.5.8",
      "@types/node": "^8.0.29",
      "@types/swagger-ui-express": "^4.1.3",
      "jest": "^28.1.2",
      "nodemon": "^2.0.15",
